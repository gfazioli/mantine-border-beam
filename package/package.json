{
  "name": "@gfazioli/mantine-border-beam",
  "version": "0.1.0",
  "description": "Mantine Border Beam is a React component that allows you to create a animated beam of light which travels along the border of its container.",
  "homepage": "https://gfazioli.github.io/mantine-border-beam/",
  "packageManager": "yarn@4.0.1",
  "license": "MIT",
  "author": "Giovambattista Fazioli <giovambattista.fazioli@gmail.com>",
  "keywords": [
    "extension",
    "mantine",
    "border",
    "effect",
    "react",
    "react-component",
    "beam",
    "typescript"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./styles.css": "./dist/styles.css",
    "./styles.layer.css": "./dist/styles.layer.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gfazioli/mantine-border-beam.git"
  },
  "workspaces": [
    "docs"
  ],
  "peerDependencies": {
    "@mantine/core": ">=7.0.0",
    "@mantine/hooks": ">=7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "bugs": "https://github.com/gfazioli/mantine-border-beam/issues"
}
